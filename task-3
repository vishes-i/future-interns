  <script>
    // display all categori Button
async function displayCategorieBtn(){
  const response = await fetch('https://openapi.programming-hero.com/api/phero-tube/categories');
  const obj = await response.json();
  const datas = obj.categories;
  for(const data of datas){
    const nav = document.getElementById('categorie-section');
    const button = document.createElement("button");
    button.setAttribute('onclick', `fetchByCategorie(${data.category_id})`)
    button.innerText = data.category;
    button.classList.add('button-style');
    nav.appendChild(button);
    // nav.innerHTML = `
    // <button class="button-style" onclick="fetchByCategorie(${data.category_id})">${data.category}</button>
    // `
  }
}
// display video
async function displayVideo(){
  const response = await fetch('https://openapi.programming-hero.com/api/phero-tube/videos');
  const obj = await response.json();
  const datas = await obj.videos
  for(const data of datas){
    const main = document.getElementById('main')
    const div = document.createElement('div')
    div.innerHTML = 
      `
      <div class="card">
      <div class="card-image-container">
        <img src="${data.thumbnail}" alt="youtube video">
      </div>
        <p class="card-title">${data.title}</p>
        <p class="card-des">${data.description}</p>
      </div>
      `
    main.appendChild(div)    
  }
}
// display video when click the categorie button
async function fetchByCategorie(category_id){
  const res = await fetch(`https://openapi.programming-hero.com/api/phero-tube/category/${category_id}`)
  const data = await res.json();
  // console.log(data)
  const main = document.getElementById('main');
  main.innerHTML = '';
  for(const datum of data.category){
    const div = document.createElement('div')
    div.innerHTML = 
      `
      <div class="card">
      <div class="card-image-container">
        <img src="${datum.thumbnail}" alt="youtube video">
      </div>
        <p class="card-title">${datum.title}</p>
        <p class="card-des">${datum.description}</p>
      </div>
      `
    main.appendChild(div)
  }
  if(data.category.length === 0){
    main.innerHTML= 
      `
      <h2>No video Found:(</h2>
      `
  }
}



displayCategorieBtn()
displayVideo()
  </script>
</body>

