         <script >
            function youtube() {
 
 "use strict";
 
 var
     title = document.getElementById('entertitle').value,
     name = document.getElementById('entername').value,
     veiws = document.getElementById('enterveiws').value,
     date = document.getElementById('enterdate').value;
     
     
     document.getElementById('title').innerHTML = title;
     document.getElementById('name').innerHTML = name;
     document.getElementById('views').innerHTML = veiws + "K" + " veiws";
     document.getElementById('date').innerHTML = date + " month" + " ago";
     
     
     if (title == "" || veiws == "" || name == "" || date == ""){
       document.getElementById('warning').innerHTML = "the field is empty";
     } else{
       document.getElementById('warning').innerHTML = "";
     }
}
function theAge() {
   
   "use strict";
   
   var  age = document.getElementById('fill').value,
   
        ruslt = age * 365,
        
        massage = document.getElementById('test');
   
   
   
       
       if(age === "") {
           
           document.getElementById('test').innerHTML = ruslt ;
           
       } else if(isNaN(age)) {
           
           massage.innerHTML = "You must be Enter a number" ;
           
       } else if( age === "0") {
           
           massage.innerHTML = "You must be enter a number else zero" ;
           
       } else if( age < 0 ) {
           
           massage.innerHTML = "you must be enter a positve number" ;
           
       }else {
           
           massage.innerHTML = "Your age by days is " + ruslt + " Day" ;
       }
       
}

         </script>
