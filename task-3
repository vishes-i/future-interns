<body style="background-image: url('https://i.pinimg.com/originals/51/4d/92/514d9258d87e107aeaf0b45be8c86f02.gif');">

    <header style="background-image: url('https://th.bing.com/th/id/OIP.41UPOQtvMwhvVFYpGNFX1wAAAA?rs=1&pid=ImgDetMain');">
        <h1>YouTube</h1>
        <input type="file" id="uploadVideo" accept="video/*">
        <button onclick="uploadVideo()">Upload Video</button>
    </header>

    <main id="videoContainer">
        <!-- Videos will be displayed here -->
    </main>

    <script >
        document.addEventListener("DOMContentLoaded", loadVideos);

function uploadVideo() {
    let videoFile = document.getElementById("uploadVideo").files[0];

    if (videoFile) {
        let videoUrl = URL.createObjectURL(videoFile);
        saveVideo(videoUrl);
    } else {
        alert("Please select a video file!");
    }
}

function saveVideo(videoUrl) {
    let videos = JSON.parse(localStorage.getItem("videos")) || [];
    videos.push(videoUrl);
    localStorage.setItem("videos", JSON.stringify(videos));
    loadVideos();
}

function loadVideos() {
    let videoContainer = document.getElementById("videoContainer");
    videoContainer.innerHTML = "";
    let videos = JSON.parse(localStorage.getItem("videos")) || [];

    videos.forEach(videoUrl => {
        let videoElement = document.createElement("video");
        videoElement.src = videoUrl;
        videoElement.controls = true;
        videoElement.classList.add("video-item");
        videoContainer.appendChild(videoElement);
    });
}
    </script>
</body>
