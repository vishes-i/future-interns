    <script src="script.js">
      
const firebaseConfig = {
    apiKey: "YOUR_API_KEY",
    authDomain: "YOUR_AUTH_DOMAIN",
    projectId: "YOUR_PROJECT_ID",
    storageBucket: "YOUR_STORAGE_BUCKET",
    messagingSenderId: "YOUR_MESSAGING_SENDER_ID",
    appId: "YOUR_APP_ID"
};

// Initialize Firebase
firebase.initializeApp(firebaseConfig);
const db = firebase.firestore();

// Send Message
function sendMessage() {
    let messageInput = document.getElementById("messageInput");
    if (messageInput.value.trim() !== "") {
        db.collection("messages").add({
            text: messageInput.value,
            timestamp: firebase.firestore.FieldValue.serverTimestamp()
        });
        messageInput.value = "";
    }
}

// Load Messages
db.collection("messages").orderBy("timestamp")
    .onSnapshot(snapshot => {
        let messagesDiv = document.getElementById("messages");
        messagesDiv.innerHTML = "";
        snapshot.forEach(doc => {
            let message = document.createElement("p");
            message.textContent = doc.data().text;
            messagesDiv.appendChild(message);
        });
    });
// Firebase Configuration
const firebaseConfig = {
    apiKey: "YOUR_API_KEY",
    authDomain: "YOUR_AUTH_DOMAIN",
    projectId: "YOUR_PROJECT_ID",
    storageBucket: "YOUR_STORAGE_BUCKET",
    messagingSenderId: "YOUR_MESSAGING_SENDER_ID",
    appId: "YOUR_APP_ID"
};

// Initialize Firebase
firebase.initializeApp(firebaseConfig);
const db = firebase.firestore();

// OpenAI API Key
const OPENAI_API_KEY = "YOUR_OPENAI_API_KEY";

// Send Message
function sendMessage() {
    let messageInput = document.getElementById("messageInput");
    let messageText = messageInput.value.trim();

    if (messageText !== "") {
        db.collection("messages").add({
            text: messageText,
            sender: "user",
            timestamp: firebase.firestore.FieldValue.serverTimestamp()
        });

        messageInput.value = "";
        getAIResponse(messageText);
    }
}

// Get AI Response
async function getAIResponse(userMessage) {
    const response = await fetch("https://api.openai.com/v1/chat/completions", {
        method: "POST",
        headers: {
            "Content-Type": "application/json",
            "Authorization": `Bearer ${OPENAI_API_KEY}`
        },
        body: JSON.stringify({
            model: "gpt-3.5-turbo",
            messages: [{ role: "user", content: userMessage }]
        })
    });

    const data = await response.json();
    const aiReply = data.choices[0].message.content;

    db.collection("messages").add({
        text: aiReply,
        sender: "AI",
        timestamp: firebase.firestore.FieldValue.serverTimestamp()
    });
}

// Load Messages
db.collection("messages").orderBy("timestamp")
    .onSnapshot(snapshot => {
        let messagesDiv = document.getElementById("messages");
        messagesDiv.innerHTML = "";
        snapshot.forEach(doc => {
            let message = document.createElement("p");
            let sender = doc.data().sender === "AI" ? "🤖 AI: " : "😊 You: ";
            message.textContent = sender + doc.data().text;
            messagesDiv.appendChild(message);
        });
    });



    </script>
</body>
                           </html>
                           
    <script>
        let channelName, niche, videoType, videoTitle;

function nextStep(step) {
    // Show the next step
    document.getElementById('step' + (step + 1)).style.display = 'block';

    // Capture the inputs for each step
    if (step === 1) {
        channelName = document.getElementById('channelName').value;
    } else if (step === 2) {
        niche = document.getElementById('niche').value;
    } else if (step === 3) {
        videoType = document.getElementById('videoType').value;
    } else if (step === 4) {
        videoTitle = document.getElementById('videoTitle').value;
    }
}

// This function will not hide any previous steps, and instead will only show the results.
function showResults() {
    // Calculate simulation results
    const views = Math.floor(Math.random() * 10000);
    const likes = Math.floor(views * 0.1);
    const comments = Math.floor(views * 0.05);
    const subscribers = Math.floor(views * 0.01);

    // Display the results
    const resultMessage = `
        Channel: ${channelName}  
        Niche: ${niche}  
        Video Type: ${videoType}  
        Title: ${videoTitle}

        Video Performance (After 1 Week):
        - Views: ${views}
        - Likes: ${likes}
        - Comments: ${comments}
        - New Subscribers: ${subscribers}
    `;

    document.getElementById('resultMessage').innerText = resultMessage;
    document.getElementById('results').style.display = 'block';
}

    </script>
</body>
</html>

